<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">

  <!-- VIEW -->
  <grid name="domain-grid" title="Domains">
    <field name="alias" />
    <field name="server" />
  </grid>

  <form name="domain-form" title="Domain" cols="4">
  	<field name="alias" />
  	<field name="server" form-view="server-form" grid-view="server-grid"/>
  </form>

  <!-- MENU -->
  <menuitem name="menu-domain" title="Domains" action="action-view-domain" parent="menu-network-root" priority="99"/>

  <action-view name="action-view-domain" title="Domains" model="com.axelor.apps.network.db.Domain" icon="img/16px/folder-network.png" >
  	  <view type="grid" name="domain-grid"/>
  	  <view type="form" name="domain-form"/>
  </action-view>
    
</object-views>
